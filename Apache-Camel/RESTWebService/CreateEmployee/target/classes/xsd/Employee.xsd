<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schema/ZUnitTest/Schema.xsd"
	 targetNamespace="http://www.tibco.com/schema/ZUnitTest/Schema.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="Emp">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FirstName" minOccurs="0"/>
				<xs:element ref="LastName" minOccurs="0"/>
				<xs:element ref="Age" minOccurs="0"/>
				<xs:element ref="Designation" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FirstName" type="xs:string"/>
	<xs:element name="LastName" type="xs:string"/>
	<xs:element name="Age" type="xs:int"/>
	<xs:element name="Designation" type="xs:string"/>
</xs:schema>